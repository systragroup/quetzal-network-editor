{
  "compilerOptions": {
    "allowJs": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "useDefineForClassFields": true,
    "types": [
      "node"
    ],
    "lib": [
      "esnext",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@src/*": [
        "./src/*"
      ],
      "@comp/*": [
        "./src/components/*"
      ],
      "@scss/*": [
        "./src/scss/*"
      ],
      "@page/*": [
        "./src/pages/*"
      ],
      "@static/*": [
        "./static/*"
      ],
      "@types/*": [
        "./src/types/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
    }
  },
}